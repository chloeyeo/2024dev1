<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.study.spring.dao.ITransaction1Dao">
		<select id="pay">
			insert into transaction1 (consumerId, amount)
			values (#{param1}, #{param2})
			<!--Do not add semicolon(;) at the end becuase we don't want a return value/result back
			as we're only doing an insert operation-->
		</select>
		
	</mapper>